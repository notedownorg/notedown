{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "notedown",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_content"
      }
    },
    "_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "heading"
        },
        {
          "type": "SYMBOL",
          "name": "wikilink"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "heading": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "level",
            "content": {
              "type": "REPEAT1",
              "content": {
                "type": "STRING",
                "value": "#"
              }
            }
          },
          {
            "type": "PATTERN",
            "value": "\\s+"
          },
          {
            "type": "FIELD",
            "name": "content",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "text"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "wikilink"
                  }
                ]
              }
            }
          }
        ]
      }
    },
    "wikilink": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "[["
            },
            {
              "type": "FIELD",
              "name": "target",
              "content": {
                "type": "SYMBOL",
                "name": "wikilink_target"
              }
            },
            {
              "type": "STRING",
              "value": "]]"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "[["
            },
            {
              "type": "FIELD",
              "name": "target",
              "content": {
                "type": "SYMBOL",
                "name": "wikilink_target"
              }
            },
            {
              "type": "STRING",
              "value": "|"
            },
            {
              "type": "FIELD",
              "name": "display",
              "content": {
                "type": "SYMBOL",
                "name": "wikilink_display"
              }
            },
            {
              "type": "STRING",
              "value": "]]"
            }
          ]
        }
      ]
    },
    "wikilink_target": {
      "type": "PATTERN",
      "value": "[^\\|\\]]+"
    },
    "wikilink_display": {
      "type": "PATTERN",
      "value": "[^\\]]+"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\[\\]#\\n]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}